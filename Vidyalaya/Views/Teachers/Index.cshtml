@model IEnumerable<Vidyalaya.Models.Teachers>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TSubject)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TStandard)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TSubject)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TStandard)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
